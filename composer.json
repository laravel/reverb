{
    "name": "laravel/reverb",
    "description": "Laravel Reverb brings real-time WebSocket communication for Laravel applications.",
    "keywords": [
        "laravel",
        "websocket",
        "websockets",
        "real-time"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Dixon",
            "email": "hello@joedixon.co.uk"
        }
    ],
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.241",
        "cboden/ratchet": "^0.4.4",
        "clue/redis-react": "^2.6",
        "illuminate/cache": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/redis": "^10.0",
        "illuminate/support": "^10.0",
        "nesbot/carbon": "^2.64"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.3",
        "ratchet/pawl": "^0.4.1",
        "react/async": "^4.0",
        "react/http": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Reverb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Reverb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Reverb\\ManagerProvider",
                "Laravel\\Reverb\\ServiceProvider"
            ],
            "aliases": {
                "Output": "Laravel\\Reverb\\Output"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}